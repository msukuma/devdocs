<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:</string>
    </dict>
    <key>Label</key>
    <string>com.devdocs</string>
    <key>Program</key>
    <string>/path/to/devdocs/devdocs.sh</string>
    <key>RunAtLoad</key>
    <true />
    <key>KeepAlive</key>
    <true />
    <key>WorkingDirectory</key>
    <string>/path/to/devdocs</string>
    <key>UserName</key>
    <string>john</string>
    <key>GroupName</key>
    <string>admin</string>
    <key>InitGroups</key>
    <true />
  </dict>
</plist>